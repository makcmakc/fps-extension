{
  "manifest_version": 2,
  "name": "FPS",
  "description": "FPS Extension",
  "version": "0.1",
  // "browser_action": {
  //   "default_icon": ""
  // },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["main.js"],
      "css": ["style.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}